journalbeat.inputs:
- paths: []
  #id: ""
  backoff: 1s
  # The maximum number of seconds to wait before attempting to read again from journals.
  max_backoff: 15s

  seek: cursor
  cursor_seek_fallback: head

output.kafka:
  hosts:
    - "<%= @endpoint %>:9093"
  topic: oslogs
  version: "2.0.0"
  ssl.enabled: true
  username: "$ConnectionString"
  password: "<%= @connection_string %>"
  compression: none

processors:
  - add_host_metadata: ~
  - add_cloud_metadata: ~
  - drop_fields:
      fields: ["ecs", "log", "@metadata"]
      ignore_missing: true

# Available log levels are: error, warning, info, debug
logging:
  level: info
  ecs: true
  metrics:
    enabled: false
  
